<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkListStore" id="lsSongs">
    <columns>
      <!-- column-name Filename -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">blabla</col>
      </row>
    </data>
  </object>
  <object class="GtkWindow" id="winPlayer">
    <property name="width_request">400</property>
